{
  "name": "temnoappe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "run-script-os",
    "pretest:win32": "SET NODE_ENV=test&&knex migrate:rollback --all&&knex migrate:latest",
    "pretest:darwin:linux": "export NODE_ENV=test&&knex migrate:rollback --all&&knex migrate:latest",
    "test": "run-script-os",
    "test:win32": "SET NODE_ENV=test&&jest --forceExit",
    "test:darwin:linux": "export NODE_ENV=test&&jest --forceExit",
    "predev": "run-script-os",
    "predev:win32": "SET NODE_ENV=development&&knex migrate:rollback --all&&knex migrate:latest",
    "predev:darwin:linux": "export NODE_ENV=development&&knex migrate:rollback --all&&knex migrate:latest",
    "dev": "run-script-os",
    "dev:win32": "SET NODE_ENV=development&&nodemon --exec ts-node --files src/server.ts",
    "dev:darwin:linux": "export NODE_ENV=development&&nodemon --exec ts-node --files src/server.ts",
    "dev-without-rollback": "run-script-os",
    "dev-without-rollback:win32": "SET NODE_ENV=development&&knex migrate:latest&&nodemon --exec ts-node --files src/app.ts",
    "dev-without-rollback:darwin:linux": "export NODE_ENV=development&&knex migrate:latest&&nodemon --exec ts-node --files src/app.ts",
    "build": "tsc",
    "start": "run-script-os",
    "start:win32": "SET NODE_ENV=production&&knex migrate:latest&&pm2 start pm2.json",
    "start:darwin:linux": "export NODE_ENV=production&&knex migrate:latest&&pm2 start pm2.json",
    "production": "npm run build&&pm2 flush&&npm start",
    "stop-production": "pm2 stop pm2.json",
    "show-logs": "pm2 logs --lines 1000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^27.0.3",
    "@types/supertest": "^2.0.11",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.4.3",
    "knex": "^0.95.14",
    "mysql2": "^2.3.3",
    "run-script-os": "^1.1.6",
    "ts-jest": "^27.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
